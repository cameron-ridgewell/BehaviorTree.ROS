<?xml version="1.0"?>
<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <Sequence>
            <Action ID="AddTwoInts" first_int="3" second_int="4" service_name="AddTwoInts" sum="{add_two_result}"/>
            <Action ID="PrintValue" message="{add_two_result}"/>
            <RetryUntilSuccesful num_attempts="4">
                <Timeout msec="300">
                    <Action ID="Fibonacci" order="5" result="{fibonacci_result}" server_name="Fibonacci"/>
                </Timeout>
            </RetryUntilSuccesful>
            <Action ID="PrintValue" message="{fibonacci_result}"/>
        </Sequence>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="AddTwoInts">
            <input_port name="first_int"/>
            <input_port name="second_int"/>
            <input_port default="AddTwoInts" name="service_name"/>
            <output_port name="sum"/>
        </Action>
        <Action ID="Fibonacci">
            <input_port name="order"/>
            <output_port name="result"/>
            <input_port default="fibonacci" name="server_name"/>
        </Action>
        <Action ID="PrintValue">
            <input_port name="message"/>
        </Action>
    </TreeNodesModel>
    <!-- ////////// -->
</root>

